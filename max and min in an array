#include <bits/stdc++.h>
using namespace std;

int main() {
	int t;
	cin>>t;
	while(t--){
	    int n;
	    cin>>n;
	    int ar[n];
	    for(int i=0; i<n; i++) cin>>ar[i];
	    int min=INT_MAX;
	    int max= INT_MIN;
	    for (int i=0; i<n; i++){
	        if (ar[i]>max) max=ar[i];
	        if (ar[i]<min) min=ar[i];
	    }
	    cout<<max<<" "<<min<<endl;
	}
	
	return 0;
}
